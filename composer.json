{
    "name": "jono/ui",
    "description": "Reusable UI components for Laravel & Livewire",
    "version": "2.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Asysadzili",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Hehe\\UI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hehe\\UI\\HeheUIServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}